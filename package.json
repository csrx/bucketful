{
  "name": "bucketful",
  "description": "Deploys opra-apps to S3 buckets",
  "main": "./lib/bucketful.js",
  "bin": {
    "bucketful": "./bin/bucketful"
  },
  "version": "0.11.0",
  "author": "Jakob Mattsson <jakob.mattsson@gmail.com> (jakobmattsson.se)",
  "repository": {
    "type": "git",
    "url": "git://github.com/jakobmattsson/bucketful.git"
  },
  "engines": {
    "node": ">=0.6.0"
  },
  "dependencies": {
    "loopia-api": ">= 0.1.4 < 0.2",
    "aws-sdk": ">= 1.3.2 < 2",
    "powerfs": ">= 0.2.3 < 0.3",
    "knox": ">= 0.4.1 < 0.5",
    "awssum": ">= 0.12.0 < 0.13",
    "q": ">= 0.9.2 < 0.10",
    "optimist": ">= 0.3.4 < 0.4",
    "wrench": ">= 1.3.9 < 2",
    "underscore": ">= 1.3.3 < 2",
    "nconf": ">= 0.6.0 < 0.7",
    "underscore.string": ">= 2.2.0 < 3"
  },
  "peerDependencies": {
    "opra": ">= 0.0.4"
  },
  "devDependencies": {
    "jscov": ">= 0.4.1 < 0.5",
    "mocha": ">= 1.8.2 < 2",
    "mocha-term-cov-reporter": ">= 0.1.0 < 0.2",
    "should": ">= 1.2.2 < 2",
    "coffee-script": ">= 1.6.2 < 2"
  },
  "scripts": {
    "coverage": "jscov --expand src .cov && JSCOV=.cov mocha --compilers coffee:coffee-script --reporter mocha-term-cov-reporter spec/*.coffee",
    "test": "mocha --compilers coffee:coffee-script spec/*.coffee",
    "pretest": "coffee -co lib src",
    "prepublish": "coffee -co lib src"
  }
}
